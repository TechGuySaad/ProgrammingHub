#include <stdio.h>

int main()
{
float farenheit;
printf("ENTER FARENHEIT VALUE :  ");
scanf("%f",&farenheit);

float celsius = (farenheit - 32)*5/9;
printf("CELSIUS IS : %.1f\n",celsius);
if(celsius>37)
{
printf("CELSIUS EXCEEDS HUMAN TEMPERATURE\n");
}
else
{
printf("CELSIUS DOESNOT EXCEEDS HUMAN TEMPERATURE");
}
return 0;
}
